! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
rofi.color-enabled: true
! State:           'bg',     'fg',     'bgalt',  'hlbg',   'hlfg'
rofi.color-normal: #282A36,  #f8f8f2,  #282a36,  #6272a4,  #f8f8f2
rofi.color-urgent: #ff5555,  #f8f8f2,  #282a36,  #6272a4,  #f8f8f2
rofi.color-active: #282A36,  #f8f8f2,  #282a36,  #6272a4,  #f8f8f2

!                  'background', 'border', 'separator'
rofi.color-window: #282a36,      #ff5555,  #ff79c6

Xcursor.size: 16

! -----------------------------------------------------------------------------
URxvt.font: xft:office code pro:size=22,\
            xft:source code pro:size=22
URxvt.scrollBar: false
URxvt.letterSpace: -1
URxvt*depth: 32
URxvt*background: [80]#282A36

! Dracula Xresources palette
*.foreground: #F8F8F2
*.background: #282A36
*.color0:     #000000
*.color8:     #4D4D4D
*.color1:     #FF5555
*.color9:     #FF6E67
*.color2:     #50FA7B
*.color10:    #5AF78E
*.color3:     #F1FA8C
*.color11:    #F4F99D
*.color4:     #BD93F9
*.color12:    #CAA9FA
*.color5:     #FF79C6
*.color13:    #FF92D0
*.color6:     #8BE9FD
*.color14:    #9AEDFE
*.color7:     #BFBFBF
*.color15:    #E6E6E6

